  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run backend:start\" \"npm run frontend:dev\"",
    "backend:bootstrap": "npm run bootstrap -w @realexperts/backend",
    "backend:start": "npm run start -w @realexperts/backend",
    "backend:start:bootstrap": "npm run start:bootstrap -w @realexperts/backend",
    "backend:extension:models:export": "npm run extension:models:export -w @realexperts/backend",
    "frontend:dev": "npm run dev -w @realexperts/frontend",
    "frontend:start": "npm run start -w @realexperts/frontend",
    "frontend:build": "npm run build -w @realexperts/frontend",
    "lint": "npm run lint -w @realexperts/frontend",
    "lint:fix": "npm run lint:fix -w @realexperts/frontend",
    "check-types": "npm run check-types -w @realexperts/frontend",
    "update": "npx npm-check-updates -ui && npx npm-check-updates -ui -ws"
  },
  "workspaces": [
    "packages/*"
  ]
