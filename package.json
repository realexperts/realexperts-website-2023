{
  "name": "realexperts-website-2023",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "concurrently": "^8.2.2",
    "npm-check-updates": "16.14.6"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"pnpm dev:backend\" \"pnpm dev:frontend\"",
    "dev:backend": "pnpm --filter @realexperts/backend dev",
    "dev:frontend": "pnpm --filter @realexperts/frontend dev",
    "lint": "pnpm --filter @realexperts/frontend lint",
    "lint:fix": "pnpm --filter @realexperts/frontend lint:fix",
    "check-types": "pnpm --filter @realexperts/frontend check-types",
    "check-updates": "pnpm update --interactive --latest",
    "export:models": "pnpm -C packages/backend exec directus models snapshot ../frontend/src/lib/types.ts"
  },
  "workspaces": [
    "packages/*"
  ]
}
